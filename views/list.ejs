<%- include("header") %>

    <div class="box">
      <h1 id = "heading" ><%= listTitle %></h1>
    </div>
    <div class="box">

        <% for (let i=0 ;i< itemsarray.length ;i++){ %>
          <div class="item">
            <input type="checkbox" >
            <p ><%= itemsarray[i] %></p>
          </div>
          <% } %>

        <form class="item" action="/" method="post">
          <input type="text" name="newItem"  placeholder = "new item">
          <button type="submit" name="listValue" value = <%= listTitle  %> >+</button>
        </form>

    </div>


<%- include("footer") %>
